{
  "manifest_version": 3,
  "name": "Woodpecker",
  "version": "1.0.0",
  "description": "Save links and posts",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "css": ["assets/css/reddit.css"],
      "js": [
        "bundle.js"
      ],
      "matches": [
        "https://www.reddit.com/*",
        "https://reddit.com/*"
      ],
      "resources": ["assets/images/acorn.png"]
    }
  ]
}